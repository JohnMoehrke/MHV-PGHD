      </searchParam>
      <searchParam>
        <name value="_count"/>
        <type value="number"/>
      </searchParam>
      <searchParam>
        <name value="_pretty"/>
        <type value="string"/>
      </searchParam>
      <searchParam>
        <name value="_format"/>
        <type value="uri"/>
      </searchParam>
    </resource>
    <interaction>
      <code value="search-system"/>
    </interaction>
  </rest>
